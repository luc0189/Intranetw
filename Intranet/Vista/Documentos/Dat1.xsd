            <SelectCommand>
              <DbCommand CommandType="Text" ModifiedByUser="true">
                <CommandText>SELECT        a.codigo, a.detalle, cd.codbarra, CAST(pc.valormiva AS int) AS valormiva, CAST(a.peso AS int) AS peso, CAST(condiprom.vrveneficio AS int) AS descuento
FROM            articulo AS a LEFT OUTER JOIN
                         artspromo AS aprom ON a.codigo = aprom.articuloID LEFT OUTER JOIN
                         condicionpromo AS condiprom ON aprom.promocionID = condiprom.promocionID LEFT OUTER JOIN
                         promocion AS promo ON condiprom.promocionID = promo.id LEFT OUTER JOIN
                         codbar AS cd ON cd.articuloID = a.codigo LEFT OUTER JOIN
                         precio AS pc ON pc.articuloID = a.codigo AND (pc.presentacionID = cd.presentacionID OR
                         pc.presentacionID IS NULL AND cd.presentacionID IS NULL)
WHERE        (a.inactivo = 0) AND (cd.codbarra = @Param1) AND (pc.listprecioID = 'BX1')</CommandText>
