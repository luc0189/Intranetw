<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner"><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="origen"><ConnectionProperties><DataProvider>SQL</DataProvider><ConnectString>Data Source=192.168.1.113,7433;Initial Catalog=supermio</ConnectString></ConnectionProperties><rd:SecurityType>DataBase</rd:SecurityType><rd:DataSourceID>e4ee21d8-618e-4037-aa82-bfac713c1918</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="empresa"><Query><DataSourceName>origen</DataSourceName><QueryParameters><QueryParameter Name="@empresaID"><Value>=Parameters!empresaID.Value</Value></QueryParameter></QueryParameters><CommandText>
          SELECT   *
          FROM  vwEmpresa where (@empresaID is null or id in(select pvalor from dbo.fnSplitTable(@empresaID,',')))
        </CommandText><rd:UseGenericDesigner>true</rd:UseGenericDesigner></Query><Fields><Field Name="ano"><DataField>ano</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="mes"><DataField>mes</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="nombrec"><DataField>nombrec</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="nombre"><DataField>nombre</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="nit"><DataField>nit</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="digver"><DataField>digver</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="logo"><DataField>logo</DataField><rd:TypeName>System.Byte[]</rd:TypeName></Field><Field Name="regimen"><DataField>regimen</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="direccion"><DataField>direccion</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="telefono1"><DataField>telefono1</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ciudad"><DataField>ciudad</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="dpto"><DataField>dpto</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="telmovil"><DataField>telmovil</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="replegal"><DataField>replegal</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ccreplegal"><DataField>ccreplegal</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="contador"><DataField>contador</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="cccontador"><DataField>cccontador</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="tipopuc"><DataField>tipopuc</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="telefono2"><DataField>telefono2</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="autorefreshinf"><DataField>autorefreshinf</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="ccrevfiscal"><DataField>ccrevfiscal</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="revfiscal"><DataField>revfiscal</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="tprevfiscal"><DataField>tprevfiscal</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="tpcontador"><DataField>tpcontador</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ciudadID"><DataField>ciudadID</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="email"><DataField>email</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="url"><DataField>url</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="id"><DataField>id</DataField><rd:TypeName>System.Int32</rd:TypeName></Field></Fields></DataSet><DataSet Name="items"><Query><DataSourceName>origen</DataSourceName><QueryParameters><QueryParameter Name="@cxcp"><Value>=Parameters!cxcp.Value</Value></QueryParameter><QueryParameter Name="@Fechafin"><Value>=Parameters!Fechafin.Value</Value></QueryParameter><QueryParameter Name="@terceros"><Value>=Parameters!terceros.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT  i.id as itcontabID,
					d.fecha , 
					i.terceroID,
					tercontab.nombrecompleto as nomter,
					d.observac,
					d.tipo, 
					d.numero,
					docrela.tipo as tdr,
					docrela.numero as numdocr,
					i.cuotadocr,
					case when fns.terceroID is not null  then DateDiff(Day, itfpago.fechaven , Cast(getdate() as date)) else NULL end as mora,
					i.debito, 
					i.credito,
					CASE WHEN i.valorex&gt;0 THEN i.valorex ELSE CAST(0 AS numeric(15,2)) END as debitoex,
					CASE WHEN i.valorex&lt;0 THEN ABS(i.valorex) ELSE CAST(0 AS numeric(15,2)) END as creditoex,
					i.cuentaID, 
					tercontab.idpadre,
					case when @cxcp = 1 and i.credito&lt;&gt;0 Then 1 else      -- Cuentas por pagar
						case when @cxcp = 0 and i.debito&lt;&gt;0 Then 1 else  -- Cuentas por cobrar
					2 end end as itorden,
					d.logfecreo,
					-- CASE WHEN ( ctascxcp.tipocuentaEnum='AnticipoAProveedor' OR ctascxcp.tipocuentaEnum='AnticipoDeCliente' ) Then CAST(1 as bit) else CAST(0 as bit) End as anticipo,
					cast((select top 1 case when ctas.cuentaID is not null 
							and ctas.empresaID=d.empresaID then 1 else 0 end  From ctas 
							where ctas.tipocuentaEnum in('AnticipoAProveedor','AnticipoAProveedorExte')
						and i.cuentaID like ctas.cuentaID+'%'	) as bit) anticipo,
					itfpago.fechaven,
				   coalesce(fns.saldo,0.0) as saldo,
				   (i.valorex) *-1 as saldoex,
					case when tpadre.id is not null then tpadre.nombrecompleto else null end as nomterpadre,
					td.clasedoc,
					CAST(0 as numeric(15,2)) as saldoparcial,
					docrela.vendedorID,
					CASE WHEN terdocr.id&lt;&gt;tercontab.id THEN terdocr.id ELSE CAST(NULL as nvarchar(12)) END as sucursalID,
					CASE WHEN terdocr.id&lt;&gt;tercontab.id THEN terdocr.nombrecompleto ELSE CAST(NULL as nvarchar(140)) END as nomsucursal,
					d.cobradorID,
					cheque.chequeID,
					cheque.chequenum,
					cheque.fecha as fecheque,
					i.areadatosID,
					d.empresaID,
					i.docrelaID,
					docrela.fecha as fechar
		From [documento] d inner join itcontab i on i.documentID=d.[id]
						INNER JOIN dbo.tipodoc td ON td.codigo = d.tipo
						LEFT JOIN [documento] docrela on i.docrelaID=docrela.[id]
						inner JOIN dbo.tercero tercontab on tercontab.id = i.terceroID
						left JOIN dbo.tercero terdocr ON terdocr.id = docrela.terceroID1
						----- Esta relación es para traer el idpadre o sucursal si existe(si idpadre no es null, es una sucursal)
						LEFT JOIN dbo.tercero tpadre on tpadre.id = tercontab.idpadre
						LEFT JOIN itfpago on itfpago.documentID = docrela.id and itfpago.num = i.cuotadocr
						LEFT JOIN dbo.fpago on fpago.codigo = itfpago.fpagoID  -- Left Join por los anticipos, que no tienen una forma de pago como tal.
						LEFT JOIN dbo.cheque on cheque.chequeid = itfpago.chequeID
					LEFT JOIN (SELECT 	s.cuentaID,
												s.terceroID,
												s.docrelaID,
												s.cuotadocr,
												SUM(s.debito -  s.credito) AS saldo
										FROM    dbo.vwSaldosDoc s
											INNER JOIN (select distinct cuentaID From dbo.fn_CtasSeleccion('AnticipoAProveedor,CuentasPorPagar,CuentasPorPagarExte,AnticipoAProveedorExte',DEFAULT,DEFAULT)) ct on ct.cuentaID=s.cuentaID
										WHERE   s.fecha &lt;= @Fechafin	
										GROUP BY s.cuentaID,
												s.terceroID,
												s.docrelaID,
												s.tdr,
												s.numdocr,
												s.cuotadocr
										HAVING SUM(s.debito-s.credito)&lt;&gt;0
									) as fns ON  fns.terceroID = i.terceroID and fns.cuentaID=i.cuentaID and fns.docrelaID = i.docrelaID and fns.cuotadocr = i.cuotadocr and fns.saldo&lt;&gt;0
					INNER JOIN (select distinct cuentaID, empresaID From dbo.fn_CtasSeleccion('AnticipoAProveedor,CuentasPorPagar,CuentasPorPagarExte,AnticipoAProveedorExte',DEFAULT,DEFAULT) 
						group by cuentaID,empresaID
								) as ctascxcp  ON ( i.cuentaID = ctascxcp.cuentaID ) -- Cuentas por cobrar y por pagar
		where @cxcp=1 
		AND d.anulado=0 and d.fecha &lt;=@Fechafin
		AND (@terceros is null or tercontab.id =@terceros)
order by fechar desc</CommandText><rd:UseGenericDesigner>true</rd:UseGenericDesigner></Query><Fields><Field Name="itcontabID"><DataField>itcontabID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="fecha"><DataField>fecha</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="terceroID"><DataField>terceroID</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="nomter"><DataField>nomter</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="observac"><DataField>observac</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="tipo"><DataField>tipo</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="numero"><DataField>numero</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="tdr"><DataField>tdr</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="numdocr"><DataField>numdocr</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="cuotadocr"><DataField>cuotadocr</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="mora"><DataField>mora</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="debito"><DataField>debito</DataField><rd:TypeName>System.Decimal</rd:TypeName></Field><Field Name="credito"><DataField>credito</DataField><rd:TypeName>System.Decimal</rd:TypeName></Field><Field Name="debitoex"><DataField>debitoex</DataField><rd:TypeName>System.Decimal</rd:TypeName></Field><Field Name="creditoex"><DataField>creditoex</DataField><rd:TypeName>System.Decimal</rd:TypeName></Field><Field Name="cuentaID"><DataField>cuentaID</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="idpadre"><DataField>idpadre</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="itorden"><DataField>itorden</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="logfecreo"><DataField>logfecreo</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="anticipo"><DataField>anticipo</DataField><rd:TypeName>System.Boolean</rd:TypeName></Field><Field Name="fechaven"><DataField>fechaven</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="saldo"><DataField>saldo</DataField><rd:TypeName>System.Decimal</rd:TypeName></Field><Field Name="saldoex"><DataField>saldoex</DataField><rd:TypeName>System.Decimal</rd:TypeName></Field><Field Name="nomterpadre"><DataField>nomterpadre</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="clasedoc"><DataField>clasedoc</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="saldoparcial"><DataField>saldoparcial</DataField><rd:TypeName>System.Decimal</rd:TypeName></Field><Field Name="vendedorID"><DataField>vendedorID</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="sucursalID"><DataField>sucursalID</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="nomsucursal"><DataField>nomsucursal</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="cobradorID"><DataField>cobradorID</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="chequeID"><DataField>chequeID</DataField><rd:TypeName>System.Guid</rd:TypeName></Field><Field Name="chequenum"><DataField>chequenum</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="fecheque"><DataField>fecheque</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="areadatosID"><DataField>areadatosID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="empresaID"><DataField>empresaID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="docrelaID"><DataField>docrelaID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="fechar"><DataField>fechar</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field></Fields></DataSet></DataSets><ReportSections><ReportSection><Body><ReportItems><Tablix Name="Tablix1"><TablixBody><TablixC